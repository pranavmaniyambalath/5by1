{
  "name": "5by1",
  "version": "0.0.1",
  "description": "5by1 server application",
  "keywords": [
    "loopback-application",
    "loopback",
    "5by1"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "16.*",
    "yarn": "1.*"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t 5by1 .",
    "docker:run": "docker run -p 3000:3000 -d 5by1",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "start:watch": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node -r source-map-support/register .\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build",
    "lb4": "lb4"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Prince Francis <princekfrancis@gmail.com>",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@aws-sdk/hash-node": "3.78.0",
    "@aws-sdk/protocol-http": "3.78.0",
    "@aws-sdk/s3-request-presigner": "3.100.0",
    "@aws-sdk/url-parser": "3.78.0",
    "@loopback/authentication": "7.3.2",
    "@loopback/authorization": "0.9.2",
    "@loopback/boot": "3.4.2",
    "@loopback/core": "2.16.2",
    "@loopback/repository": "3.7.1",
    "@loopback/rest": "10.0.0",
    "@loopback/rest-explorer": "3.3.2",
    "@loopback/service-proxy": "3.2.2",
    "@shared": "link:../shared/dist",
    "bcryptjs": "2.4.3",
    "canvas": "2.10.1",
    "dayjs": "1.10.6",
    "dotenv": "10.0.0",
    "jsonwebtoken": "8.5.1",
    "loopback-connector-mongodb": "5.2.3",
    "multer": "1.4.3",
    "tslib": "2.0.0",
    "xlsx": "^0.17.4"
  },
  "devDependencies": {
    "@loopback/build": "7.0.0",
    "@loopback/cli": "2.21.2",
    "@loopback/testlab": "3.4.2",
    "@types/accepts": "1.3.5",
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "8.5.5",
    "@types/multer": "1.4.7",
    "@types/node": "10.17.60",
    "source-map-support": "0.5.19",
    "tsc-watch": "4.5.0",
    "typescript": "4.3.4"
  }
}
